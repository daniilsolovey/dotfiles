URxvt.font: xft:UbuntuMono-Regular:size=14,style=regular, xft:Iosevka:size=14,style=regular
URxvt.boldFont: xft:UbuntuMono:size=14,style=bold, xft:Iosevka:size=14,style=bold
URxvt.italicFont: xft:UbuntuMono:size=14,style=italic, xft:Iosevka:size=14,style=italic
URxvt.boldItalicFont: xft:UbuntuMono:size=14,style=bold, xft:Iosevka:size=14,style=bold
Xft.dpi: 96 

URxvt.scrollBar: false

URxvt.perl-lib: /home/operator/.urxvt/ext/perl


URxvt.perl-ext-common: clipboard,default


URxvt.keysym.Mod4-c: perl:clipboard:copy
URxvt.keysym.Mod4-v: perl:clipboard:paste
URxvt.clipboard.copycmd:  clipboard-copy
URxvt.clipboard.pastecmd: xsel --output --clipboard

URxvt*color0: #424242
